#!/bin/bash

set -e

REPO_ROOT_DIR="$(dirname $( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null && pwd ))"

cd ${REPO_ROOT_DIR}

cat <<EOF
export AWS_ACCESS_KEY_ID="$(terraform output -state=users/tf.state prod_access_key)"
export AWS_SECRET_ACCESS_KEY="$(terraform output -state=users/tf.state prod_secret_key | base64 --decode | keybase pgp decrypt)"
EOF
